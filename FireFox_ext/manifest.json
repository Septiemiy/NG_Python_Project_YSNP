{
    "manifest_version": 2,
    "name": "YSNP",
    "version": "1.0",
  
    "description": "Blocking ads on websites for the firefox browser",

    "permissions": ["activeTab", "file://*/*"],

    "icons": {
      "48": "icons/png-adsBlocker-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["adsBlocker.js"]
      }
    ]
}
  